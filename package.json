{
    "name": "primebrick-sdk",
    "version": "0.148.0",
    "description": "PrimeBrick SDK",
    "main": "dist/index.js",
    "typings": "dist/@types/index.d.ts",
    "author": "Michael Sogos",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/michaelsogos/primebrick-sdk.git"
    },
    "files": [
        "package.json",
        "LICENSE",
        "dist/**"
    ],
    "bugs": {
        "url": "https://github.com/michaelsogos/primebrick-sdk/issues",
        "email": "michael.sogos@gurustudioweb.it"
    },
    "homepage": "https://github.com/michaelsogos/primebrick-sdk#readme",
    "keywords": [
        "primebrick",
        "sdk"
    ],
    "engines": {
        "node": ">=14.12.0"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build & copyfiles -f ./src/@types/*.d.ts ./dist/@types/",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "migration:create": "typeorm migration:generate -n coordinator_schema_update & nest build",
        "migration:run": "typeorm migration:run"
    },
    "dependencies": {
        "@nestjs/common": "8.0.4",
        "@nestjs/config": "1.0.0",
        "@nestjs/core": "8.0.4",
        "@nestjs/microservices": "8.0.4",
        "@nestjs/typeorm": "8.0.1",
        "jsonwebtoken": "8.5.1",
        "nats": "2.1.0",
        "papaparse": "5.3.1",
        "pg": "8.6.0",
        "reflect-metadata": "0.1.13",
        "rxjs": "7.2.0",
        "typeorm": "0.2.34",
        "winston": "3.3.3",
        "winston-daily-rotate-file": "4.5.5"
    },
    "devDependencies": {
        "@nestjs/cli": "8.0.2",
        "@types/express": "4.17.13",
        "@types/jsonwebtoken": "8.5.4",
        "@types/papaparse": "5.2.6",
        "@typescript-eslint/eslint-plugin": "4.28.3",
        "@typescript-eslint/parser": "4.28.3",
        "copyfiles": "2.4.1",
        "eslint": "7.31.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-import": "2.23.4",
        "prettier": "2.3.2",
        "rimraf": "3.0.2",
        "typescript": "4.3.5"
    }
}
