        "jsonwebtoken": "8.5.1",
        "nats": "1.4.12",
        "papaparse": "5.3.0",
        "pg": "8.5.0",
        "reflect-metadata": "0.1.13",
        "rxjs": "6.6.3",
        "typeorm": "0.2.29",
        "winston": "3.3.3",
        "winston-daily-rotate-file": "4.5.0"
    },
    "devDependencies": {
        "@nestjs/cli": "7.5.1",
        "@types/express": "4.17.7",
        "@types/jsonwebtoken": "8.5.0",
